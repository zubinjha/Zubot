{
    "timezone": "America/New_York",
    "home_location": {
        "lat": 40.0931,
        "lon": -83.0170,
        "city": "Worthington",
        "region": "Ohio",
        "country": "USA",
        "timezone": "America/New_York"
    },
    "weather": {
        "provider": "open-meteo",
        "base_url": "https://api.open-meteo.com/v1/forecast",
        "temperature_unit": "fahrenheit",
        "wind_speed_unit": "mph",
        "precipitation_unit": "inch",
        "timeout_sec": 10
    },
    "web_search": {
        "provider": "brave",
        "base_url": "https://api.search.brave.com/res/v1/web/search",
        "brave_api_key": "XXXXXX",
        "site": "https://api.search.brave.com/",
        "timeout_sec": 10
    },
    "has_data": {
        "api_key": "XXXXXX",
        "site": "https://hasdata.com/",
        "base_url": "https://api.hasdata.com",
        "timeout_sec": 15
    },
    "web_fetch": {
        "timeout_sec": 10,
        "max_chars": 20000,
        "user_agent": "Zubot"
    },
    "memory": {
        "session_event_logging_enabled": false,
        "autoload_summary_days": 2
    },
    "agent_loop": {
        "max_concurrent_workers": 3
    },
    "filesystem": {
        "default_access": "deny",
        "allow_read": [
            "**"
        ],
        "allow_write": [
            "memory/**",
            "outputs/**"
        ],
        "deny": [
            "config/config.json",
            ".git/**",
            ".venv/**"
        ]
    },
    "default_model_alias": "medium",
    "model_providers":{
        "openrouter":{
            "alias": "openrouter",
            "apikey": "XXXXXX",
            "site": "https://openrouter.ai/",
            "base_url": "https://openrouter.ai/api/v1/chat/completions",
            "timeout_sec": 30,
            "retry_attempts": 3,
            "retry_backoff_ms": 400,
            "referer": "https://your-app.example",
            "app_title": "Zubot"
        }
    },
    "models":{
        "gpt5_nano": {
            "name": "GPT-5 Nano",
            "alias": "low",
            "provider": "openrouter",
            "endpoint": "openai/gpt-5-nano",
            "max_context_tokens": 400000,
            "max_output_tokens": 128000
        },
        "gpt5_mini": {
            "name": "GPT-5 Mini",
            "alias": "medium",
            "provider": "openrouter",
            "endpoint": "openai/gpt-5-mini",
            "max_context_tokens": 400000,
            "max_output_tokens": 128000
        },
        "gpt5": {
            "name": "GPT-5",
            "alias": "high",
            "provider": "openrouter",
            "endpoint": "openai/gpt-5",
            "max_context_tokens": 400000,
            "max_output_tokens": 128000
        }
    }
}
